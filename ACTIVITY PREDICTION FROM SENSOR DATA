   "outputs": [],
   "source": [
    "data_s1_f = data_from_path(r\"C:\\Users\\shamm\\Downloads\\DATA(ACTIVITY RECOGNITION)\\S1_Dataset\\Female\")\n",
    "data_s1_f['gender'] = 0\n",
    "data_s1_f['RFID']= 4"
   ]
